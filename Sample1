import pandas as pd
import re

# Read the CSV file
file_path = 'your_file.csv'  # Replace with your actual CSV file path
df = pd.read_csv(file_path)

# Regular expression to find the value inside parentheses
pattern = r"(.*?)"

# Function to extract the value inside the parentheses
def extract_value(text):
    match = re.search(pattern, text)
    return match.group(1) if match else None

# Assuming the value is inside the 'Type' column
df['ValueInsideParentheses'] = df['Type'].apply(extract_value)

# Display the updated DataFrame with extracted values
print(df)
