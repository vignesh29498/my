import pandas as pd

my_list = ['value1', 'value2', 'value3', 'value4']

df = pd.read_csv('your_file.csv')

if len(my_list) == len(df):
    df['new_column_name'] = my_list
    df.to_csv('output_file.csv', index=False)
else:
    print("The length of the list doesn't match the number of rows in the CSV file.")

print(df.head())
