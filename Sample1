import pandas as pd

df1 = pd.read_csv('file1.csv')
df2 = pd.read_csv('file2.csv')

columns_file1 = df1[['ColumnA', 'ColumnB']]  # Adjust column names as needed
columns_file2 = df2[['ColumnC', 'ColumnD']]  # Adjust column names as needed

result = pd.concat([columns_file1, columns_file2], axis=1)
result.to_csv('output_file.csv', index=False)
