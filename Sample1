import pandas as pd
import re

df = pd.read_csv('your_file.csv')

null_values = []
data_types = []
sizes = []

def extract_data(value):
    null_value = "NULL" if "NULL" in value else None
    datatype_match = re.search(r'/\*([A-Z]+)', value)
    datatype = datatype_match.group(1) if datatype_match else None
    size_match = re.search(r'(\d+)', value)
    size = size_match.group(1) if size_match else None
    return null_value, datatype, size

for value in df['Value']:
    null_value, datatype, size = extract_data(str(value))
    if null_value is not None and datatype is not None:
        null_values.append(null_value)
        data_types.append(datatype)
        sizes.append(size)

df_result = pd.DataFrame({
    'Column_1_Null_Values': null_values,
    'Column_2_Data_Types': data_types,
    'Column_3_Sizes': sizes
})

df_result.to_csv('output_file.csv', index=False)

print(df_result.head())
