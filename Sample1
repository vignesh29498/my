import pandas as pd
import re

# Function to extract text between /* and */
def extract_between(value):
    match = re.search(r'/\*(.*?)\*/', value)
    if match:
        return match.group(1).strip()  # Return the text between /* and */
    return None

# Load the CSV file
csv_file_path = 'your_file.csv'  # Replace with your actual file path
df = pd.read_csv(csv_file_path)

# Assuming the column name is 'your_column'
df['extracted_values'] = df['your_column'].apply(extract_between)

# Display the extracted values
print(df[['your_column', 'extracted_values']])

# Save the output to a new CSV file (optional)
df.to_csv('extracted_values.csv', index=False)
