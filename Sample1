import pandas as pd

input_file = 'input.csv'
df_input = pd.read_csv(input_file)

column_to_extract = 'column_name'
extracted_column = df_input[column_to_extract]

output_file = 'output.csv'
df_output = pd.read_csv(output_file)

new_column_df = pd.DataFrame({column_to_extract: extracted_column})

if len(new_column_df) < len(df_output):
    new_column_df = new_column_df.reindex(df_output.index)

combined_df = pd.concat([new_column_df, df_output], axis=1)
combined_df.to_csv(output_file, index=False)
