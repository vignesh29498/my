import pandas as pd

def read_xfr_file(file_path):
    data = [
        {'Column1': 'Value1', 'Column2': 'Value2'},
        {'Column1': 'Value3', 'Column2': 'Value4'}
    ]
    return data

xfr_file_path = 'path_to_your_file.xfr'
xfr_data = read_xfr_file(xfr_file_path)

df = pd.DataFrame(xfr_data)
df.to_excel('output_file.xlsx', index=False)
