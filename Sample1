import pandas as pd

# Load the full database CSV file
db_file_path = '/path/to/full_db.csv'
db_df = pd.read_csv(db_file_path)

# Load the new sheet CSV file
new_sheet_file_path = '/path/to/new_sheet.csv'
new_sheet_df = pd.read_csv(new_sheet_file_path)

# List of values to match (replace with your list of 25 values)
values_list = ['value1', 'value2', 'value3', 'value4', 'value5', 
               'value6', 'value7', 'value8', 'value9', 'value10', 
               'value11', 'value12', 'value13', 'value14', 'value15', 
               'value16', 'value17', 'value18', 'value19', 'value20', 
               'value21', 'value22', 'value23', 'value24', 'value25']

# Create an empty list to store the results
results = []

# Iterate over the list of values and find matching rows or note "not found"
for value in values_list:
    if value in new_sheet_df.iloc[:, 0].values:
        matched_row = db_df[db_df.iloc[:, 0] == value]
        if not matched_row.empty:
            results.append(matched_row)
        else:
            not_found_row = pd.DataFrame([[value, 'not found'] + [''] * (db_df.shape[1] - 2)], columns=db_df.columns)
            results.append(not_found_row)
    else:
        not_found_row = pd.DataFrame([[value, 'not found'] + [''] * (db_df.shape[1] - 2)], columns=db_df.columns)
        results.append(not_found_row)

# Concatenate the results into a single DataFrame
final_df = pd.concat(results, ignore_index=True)

# Save the final DataFrame to a new CSV file
output_file_path = '/path/to/output_matched_rows.csv'
final_df.to_csv(output_file_path, index=False)

print(f'Matched rows have been written to "{output_file_path}".')
