import pandas as pd
import re

def extract_all_after_utf8(value):
    matches = re.findall(r'\x01(.*?)(?=\x01|$)', value)
    return [match.strip() for match in matches]

csv_file_path = 'your_file.csv'
df = pd.read_csv(csv_file_path)

column_name = 'your_column'
df['extracted_values'] = df[column_name].apply(extract_all_after_utf8)

print(df[['your_column', 'extracted_values']])
df.to_csv('modified_values.csv', index=False)
