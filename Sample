import pandas as pd

def find_labels(excel_file, search_values):
    # Load the Excel file
    df = pd.read_excel(excel_file)

    # Extract the first row as labels and the second row as values
    labels = df.iloc[0]
    values = df.iloc[1]

    # Initialize an empty list to store matching labels
    matching_labels = []

    # Iterate over the search values and check for matches in the second row
    for value in search_values:
        if value in values.values:
            # Find the index of the matching value in the second row
            index = values[values == value].index[0]
            matching_labels.append(labels[index])

    return matching_labels

# Example usage
excel_file = 'your_file.xlsx'
search_values = ['value1', 'value2', 'value3']
matched_labels = find_labels(excel_file, search_values)
print("Matched Labels:", matched_labels)
