let
    Source = Excel.Workbook(
        File.Contents("C:\Users\WV28874\Workspace\Cosmos-BABU\EXCEL\JM_E2E_Execution.xlsx"),
        null,
        true
    ),

    Sheets = Table.SelectRows(Source, each [Kind] = "Sheet"),

    Combined =
        Table.Combine(
            List.Transform(Sheets[Data], each
                Table.Skip(_,1)
            )
        )

in
    Combined
